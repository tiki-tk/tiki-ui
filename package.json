{
  "name": "rr-elm-ui",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./dist && bsb -clean-world ",
    "dev:reason": "bsb -make-world -w",
    "dev:bundle": "rollup --config rollup.config.js -w",
    "dev": "npm-run-all --parallel dev:reason dev:bundle",
    "prod:reason": "bsb -make-world",
    "prod:bundle": "rollup --config rollup.config.js",
    "prod": "npm run prod:reason && npm run prod:bundle",
    "start": "npm run prod"
  },
  "dependencies": {
    "bs-platform": "^4.0.18",
    "reason-react": "^0.5.3",
    "relude": "^0.1.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rollup": "^1.1.0",
    "rollup-plugin-node-resolve": "^4.0.0"
  }
}
